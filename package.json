{
  "name": "calculator-widget",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@lit-labs/task": "^2.0.0",
    "lit": "^2.6.1"
  },
  "scripts": {
    "build:widget": "parcel build --public-url ./ --target default",
    "serve:widget": "parcel serve src/index.html --dist-dir build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@swc/helpers": "^0.4.14",
    "parcel": "v2.8.3",
    "postcss-modules": "^6.0.0",
    "typescript": "^4.9.5"
  },
  "main": "dist/calculator.js",
  "targets": {
    "module": false,
    "default": {
      "source": [
        "src/calculator.ts",
        "src/rewiring-fonts.css",
        "src/index.html"
      ],
      "outputFormat": "global",
      "context": "browser",
      "distDir": "./dist",
      "sourceMap": false,
      "publicUrl": "./"
    }
  }
}
